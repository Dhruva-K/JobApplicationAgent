# Job Application Agent Configuration

# Neo4j Database
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password
NEO4J_DATABASE=neo4j

# LLM Configuration
# Provider: groq, ollama, or vllm
LLM_PROVIDER=groq
LLM_MODEL_NAME=llama-3.3-70b-versatile

# For Groq (recommended - fast and powerful free model)
GROQ_API_KEY=your_groq_api_key_here

# For Ollama (local inference)
# LLM_PROVIDER=ollama
# LLM_MODEL_NAME=llama3:8b
# LLM_BASE_URL=http://localhost:11434

# For vLLM (high-performance local inference)
# LLM_PROVIDER=vllm
# LLM_MODEL_NAME=llama3-8b
# LLM_BASE_URL=http://localhost:8000

# Job Search APIs
JSEARCH_API_KEY=your_jsearch_api_key
REMOTIVE_API_KEY=your_remotive_api_key
